{
  "name": "logistics-support",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "echo \"Error: no test specified\" && exit 0",
    "release": "standard-version",
    "changelog:generate": "standard-version --skip.bump --skip.commit --skip.tag",
